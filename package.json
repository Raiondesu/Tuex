{
  "name": "tuex",
  "version": "0.2.1",
  "description": "A mostly reasonable alternative to Vuex",
  "repository": "Raiondesu/Tuex",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "retest": "npm run build && npm run esmbuild && npm test",
    "preversion": "npm run retest",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "build": "tsc && uglifyjs ./dist/index.js -c toplevel,sequences=false -m reserved=['Tuex','exports'] --toplevel -o ./dist/index.min.js",
    "esmbuild": "tsc --p ./tsconfig.esm.json && uglifyjs ./dist.esm/index.js -c toplevel,sequences=false -m reserved=['Tuex','exports'] --toplevel -o ./dist.esm/index.min.js"
  },
  "keywords": [
    "Vue",
    "Vue.js",
    "Vuex",
    "Replacement",
    "Alternative",
    "Tuex",
    "JavaScript",
    "TypeScript",
    "JS",
    "TS"
  ],
  "author": "Raiondesu",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "@types/node": "^8.5.8",
    "es6-promise": "4.1.1",
    "jest": "^22.0.4",
    "ts-jest": "^22.0.1",
    "ts-node": "3.1.0",
    "typescript": "^2.6.2",
    "uglify-es": "^3.3.5",
    "uglify-js": "^3.3.5",
    "vue": "^2.5.13"
  }
}
