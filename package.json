{
  "name": "tuex",
  "version": "0.3.22",
  "description": "A simple and mostly reasonable alternative to Vuex",
  "author": "Raiondesu <a.raiondesu@gmail.com>",
  "license": "MIT",
  "repository": "Raiondesu/Tuex",
  "bugs": {
    "email": "a.raiondesu@gmail.com",
    "url": "https://github.com/Raiondesu/Tuex/issues"
  },
  "main": "dist/cjs/index.js",
  "unpkg": "dist/umd/index.min.js",
  "esnext": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "test": "jest",
    "retest": "npm run build && npm test",
    "preversion": "npm run retest",
    "postversion": "npm run build",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "build": "rollup -c ./build/index.ts"
  },
  "keywords": [
    "Vue",
    "Vue.js",
    "Vuex",
    "Replacement",
    "Alternative",
    "Tuex",
    "JavaScript",
    "TypeScript",
    "JS",
    "TS"
  ],
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.5",
    "@types/webpack": "^4.1.3",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.3",
    "ts-node": "^5.0.1",
    "typescript": "^2.8.1"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!shiva/)"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.schema.ts",
      "!src/schema/*.ts"
    ],
    "testMatch": [
      "<rootDir>/tests/**/?(*.)test.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts)$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.js$": "babel-jest"
    }
  }
}
